.more{
    display:inline-block;
    width: 150px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.more .tooltiptext {
    visibility: hidden;
    width: 100%;
    height:160px;

    background-color: black;
    color: #fff;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;

    /* Position the tooltip */
    position: absolute;
    z-index: 1;
}

.more:hover .tooltiptext {
    visibility: visible;
}
